    "coverage": "vitest run --coverage",
    "discover": "node --loader ts-node/esm scripts/bootstrap-discovery.ts"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
